{% extends 'base.html.twig' %}
{% block title %}
    {{ parent() }}
    HomePage
{% endblock %}
{% block body %}
    <div class="content">
        <h1>Our Car Offers</h1>
        <table class="table">
            <tr>
                <th>Maker</th>
                <th>Model</th>
                <th>Action</th>
                </th>
                {% for car in cars %}
            <tr>
                <td>{{ car.make }}</td>
                <td>{{ car.model }}</td>
                <td><a href="{{ url('car.show', {carId:car.id}) }}">View</a></td>
            </tr>
            {% endfor %}
        </table>
    </div>
{% endblock %}